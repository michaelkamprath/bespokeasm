#require "slu4-min64x4-asm >= 1.1.0"

.org $8000
init:
    spinit                          ; init stack pointer
main:
    phsptr message_str              ; push message string pointer onto stack
    jps _PrintPtr                   ; jump to print function

end:
    jpa _Prompt                     ; jump to prompt function

message_str:
    .cstr "Hello, World!"
