{
    "name": "BespokeASM Color Scheme",
    "globals":
    {
        "background": "rgb(34, 34, 34)",
        "foreground": "#EEEEEE",
        "caret": "#BBBBBB",
        "selection": "#294f7a",
        "line_highlight": "rgb(68, 68, 68)"
    },
    "rules":
    [
        {
            "name": "Numbers - Hex",
            "scope": "constant.numeric.integer.hexadecimal",
            "foreground": "#ffe80b"
        },
        {
            "name": "Numbers - Binary",
            "scope": "constant.numeric.integer.binary",
            "foreground": "#d6d300"
        },
        {
            "name": "Numbers - Decimal",
            "scope": "constant.numeric.integer.decimal",
            "foreground": "#fffd80"
        },
        {
            "name": "Numbers - Character",
            "scope": "constant.numeric.character",
            "foreground": "#FFE880"
        },
        {
            "name": "Labels",
            "scope": "variable.other.label",
            "foreground": "#278ed8"
        },
        {
            "name": "String Punctuation",
            "scope": "punctuation.definition.string",
            "foreground": "#ed80a2"

        },
        {
            "name": "Strings",
            "scope": "string.quoted",
            "foreground": "#aaff4d"

        },
        {
            "name": "Strings - Escaped Charaters",
            "scope": "constant.character.escape",
            "foreground": "#7ab736"
        },
        {
            "name": "Comments",
            "scope": "comment.line",
            "foreground": "#568c3b"

        },
        {
            "name": "Parameters",
            "scope": "variable.parameter",
            "foreground": "#abd7ed"

        },
        {
            "name": "Instruction Functions",
            "scope": "variable.function.instruction",
            "foreground": "#ffa83f"

        },
        {
            "name": "Macro Functions",
            "scope": "variable.function.macro",
            "foreground": "#ff883f"

        },
        {
            "name": "Variables - Language",
            "scope": "variable.language",
            "foreground": "#2ea9d2"

        },
        {
            "name": "Variables - Constant",
            "scope": "variable.other.constant",
            "foreground": "#abd7ed"
        },
        {
            "name": "Variables - Language Defined",
            "scope": "constant.language",
            "foreground": "#abedc1"
        },
        {
            "name": "Keyword - Preprocessor",
            "scope": "keyword.control.preprocessor",
            "foreground": "#c080ff"
        },
        {
            "name": "Data Types",
            "scope": "storage.type",
            "foreground": "#d6adff"

        },
        {
            "name": "Keyword - Operators",
            "scope": "keyword.operator",
            "foreground": "#cc99ff"
        },
        {
            "name": "Keyword - Other",
            "scope": "keyword.other",
            "foreground": "#c080ff"
        },
        {
            "name": "Punctuation - Preprocessor",
            "scope": "punctuation.definition.preprocessor",
            "foreground": "#ed80a2"
        },
        {
            "name": "Punctuation - Seperator",
            "scope": "punctuation.separator",
            "foreground": "#ed80a2"
        },
        {
            "name": "Punctuation - Variable",
            "scope": "punctuation.definition.variable",
            "foreground": "#ed80a2"
        },
        {
            "name": "Punctuation - Addressing Brackets",
            "scope": "punctuation.section.brackets",
            "font_style": "bold",
            "foreground": "#b72dd2"
        },
        {
            "name": "Punctuation - Addressing Double Brackets",
            "scope": "punctuation.section.double_brackets",
            "font_style": "bold",
            "foreground": "#d22d9b"
        },
        {
            "name": "Punctuation - Parenthesis",
            "scope": "punctuation.section.parens",
            "foreground": "#dc81e4"
        }
    ]
}
